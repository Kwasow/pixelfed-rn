{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "noEmit": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "src/*": ["./src/*"],
      "state/*": ["./src/state/*"],
      "@state/*": ["./src/state/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@components/*": ["./src/components/*"],
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts", "src/app/(auth)/profile/bookmarks/index.js"]
}
